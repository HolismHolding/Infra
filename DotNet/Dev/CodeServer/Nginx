server {
    listen 80;
    server_name vscode.local;
    return 301 https://$server_name$request_uri;
}

server {
    listen 443 ssl;
    server_name vscode.local;

    ssl_certificate /Temp/VsCode/Certificate.pem;
    ssl_certificate_key /Temp/VsCode/Key.pem;

    location / {
        proxy_pass http://localhost:3532;
        proxy_set_header X-Forwarded-Host  $host;
        proxy_set_header X-Forwarded-Proto $scheme;
    }
}