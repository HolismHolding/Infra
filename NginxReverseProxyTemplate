server {
    listen 80;
    server_name $Host;
    return 301 https://$server_name$request_uri;
}

server {
    listen 443 ssl;
    server_name $Host;

    ssl_certificate /HolismHolding/Infra/DevelopmentCertificate/PublicKey.crt;
    ssl_certificate_key /HolismHolding/Infra/DevelopmentCertificate/PrivateKey.key;

    location / {
        proxy_pass http://localhost:$RandomPort;
    }
}